{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "dimsemenov/magnific-popup",
                "version": "1.1.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "magnific-popup"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/dimsemenov/Magnific-Popup",
                    "reference": "1.1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "mozilla/pdf.js",
                "version": "v2.14.305",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "pdf.js"
                },
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/mozilla/pdf.js/releases/download/v2.14.305/pdfjs-2.14.305-dist.zip"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "dimsemenov/magnific-popup": "^1.1",
        "drupal/admin_toolbar": "^3.0",
        "drupal/adminimal_theme": "^1.6",
        "drupal/advpoll": "^1.2",
        "drupal/agreement": "^3.0@alpha",
        "drupal/allowed_formats": "^1.5",
        "drupal/antibot": "^1.4",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/background_image": "^2.0",
        "drupal/backup_migrate": "^5.0",
        "drupal/better_login_form_config": "^2.4",
        "drupal/better_normalizers": "^1.0@beta",
        "drupal/block_field": "^1.0@RC",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/bootstrap_sass": "^5.0",
        "drupal/captcha": "^1.2",
        "drupal/change_pwd_page": "^1.0",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_ignore": "^2.4",
        "drupal/config_split": "^1.7",
        "drupal/context": "^4.1",
        "drupal/cookiebot": "^1.0@alpha",
        "drupal/cookiepro": "^2.0",
        "drupal/core-composer-scaffold": "~9.5.0",
        "drupal/core-project-message": "~9.5.0",
        "drupal/core-recommended": "~9.5.0",
        "drupal/csv_importer": "^1.12",
        "drupal/dashboards": "^2.0",
        "drupal/default_content": "^2.0@alpha",
        "drupal/default_content_deploy": "^2.0",
        "drupal/devel": "^5.0",
        "drupal/email_registration": "^1.1",
        "drupal/entity_browser": "^2.8",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/entityreference_filter": "^1.4",
        "drupal/exclude_node_title": "^1.3",
        "drupal/feeds": "^3.0@alpha",
        "drupal/field_group": "^3.1",
        "drupal/file_delete": "^2.0",
        "drupal/file_download_link": "^2.0",
        "drupal/fillpdf": "^5.0@alpha",
        "drupal/flag": "^4.0@beta",
        "drupal/flood_control": "^2.2",
        "drupal/formblock": "^2.0@beta",
        "drupal/health_check_url": "^3.1",
        "drupal/honeypot": "^2.0",
        "drupal/iframe": "^2.12",
        "drupal/image_effects": "^3.2",
        "drupal/image_widget_crop": "^2.3",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/layout_custom_section_classes": "^1.0",
        "drupal/libraries": "^3.0@beta",
        "drupal/link_target": "1.3",
        "drupal/mailcontrol": "1.x-dev@dev",
        "drupal/masonry": "^1.2",
        "drupal/maxlength": "^2.0@RC",
        "drupal/menu_item_extras": "^2.17",
        "drupal/menu_item_role_access": "^2.0",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/module_filter": "^3.2",
        "drupal/override_node_options": "^2.6",
        "drupal/paragraphs": "^1.12",
        "drupal/passwordless": "^1.2",
        "drupal/pathauto": "^1.8",
        "drupal/pdf": "^1.1",
        "drupal/permissions_by_term": "^3.1",
        "drupal/poll": "^1.5",
        "drupal/profile_switcher": "^1.0@alpha",
        "drupal/protected_forms": "^2.0",
        "drupal/quick_node_clone": "^1.14",
        "drupal/quicktabs": "^3.0@alpha",
        "drupal/r4032login": "^2.1",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/realname": "^2.0@beta",
        "drupal/redirect": "^1.7",
        "drupal/redirect_after_login": "^2.7",
        "drupal/seckit": "^2.0",
        "drupal/shield": "^1.4",
        "drupal/smart_date": "3.6@beta",
        "drupal/status_messages": "^4.3",
        "drupal/structure_sync": "^2.0",
        "drupal/svg_image": "^1.15",
        "drupal/swiftmailer": "^2.0",
        "drupal/tablefield": "^2.2",
        "drupal/theme_switcher": "^1.2",
        "drupal/time_range": "^9.0",
        "drupal/tiny_slider": "^1.0@beta",
        "drupal/token": "^1.9",
        "drupal/twig_tweak": "^3.1",
        "drupal/unique_content_field_validation": "^1.0",
        "drupal/username_enumeration_prevention": "^1.2",
        "drupal/video_embed_field": "^2.4",
        "drupal/view_mode_switch": "^2.0",
        "drupal/view_unpublished": "^1.0",
        "drupal/views_auto_refresh": "^1.0",
        "drupal/views_conditional": "^1.4",
        "drupal/views_data_export": "^1.0",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/views_slideshow": "^4.8",
        "drupal/webform": "^6.1",
        "drupal/webform_views": "^5.0@alpha",
        "drupal/xmlsitemap": "^1.2",
        "drush/drush": "^10.6",
        "joachim-n/composer-manifest": "^1.1",
        "kint-php/kint": "^4.2",
        "mozilla/pdf.js": "^2.14",
        "oomphinc/composer-installers-extender": "^2.0",
        "symfony/css-selector": "^5.0",
        "webflo/drupal-finder": "^1.2",
        "wikimedia/composer-merge-plugin": "^2.0",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "platform": {
            "php":"8.0"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "joachim-n/composer-manifest": true,
            "oomphinc/composer-installers-extender": true,
            "wikimedia/composer-merge-plugin": true,
            "zaporylie/composer-drupal-optimizations": true
        }
    },
    "extra": {
        "patches": {
            "drupal/redirect_after_login": {
                "Headers have already been sent": "https://www.drupal.org/files/issues/2021-06-20/3214949.patch"
            },
            "drupal/backup_migrate": {
                "Failed network - error": "https://www.drupal.org/files/issues/2022-02-02/3228379-download-errors-19.patch",
                "Restore public files not creating directory":  "https://www.drupal.org/files/issues/2022-01-18/backup-migrate-restore-public-files-3245802-3.patch"
            },
            "drupal/context": {
                "TypeError: call_user_func(): Argument #1 ($callback) must be a valid callback": "https://git.drupalcode.org/project/context/-/merge_requests/23.diff"
            },
            "drupal/antibot": {
                "Undefined #id in antibot_form_alter()": "https://www.drupal.org/files/issues/2022-10-06/3313508-5.patch"
            },
            "drupal/paragraphs": {
                "No link template 'canonical' found for the 'paragraph' entity type": "https://www.drupal.org/files/issues/2021-10-18/3238515-8-paragraphs-entity-toUrl-error.patch"
            },
            "drupal/better_login_form_config": {
                "Config related issues": "https://www.drupal.org/files/issues/2020-07-28/config_related_issue.patch"
            },
            "drupal/entityreference_filter": {
                "Undefined array key title entityreference_filter.views.inc": "https://www.drupal.org/files/issues/2022-07-12/3295497_2.patch"
            },
            "drupal/permissions_by_term": {
                "PHP Deprecated: Required parameter $grantUpdate follows optional parameter $langcode": "https://www.drupal.org/files/issues/2021-10-21/php-deprecated-parameter-in-NodeAccessRecordFactory-3231556-4.patch"
            },
            "drupal/poll": {
                "Get all user votes": "patches/get-all-user-votes.patch"
            }
        },
        "merge-plugin": {
            "include": [
                "www/modules/contrib/masonry/composer.libraries.json"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "www/"
            }
        },
        "installer-paths": {
            "www/core": [
                "type:drupal-core"
            ],
            "www/libraries/{$name}": [
                "type:drupal-library"
            ],
            "www/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "www/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "www/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "www/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "www/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "www/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, you’ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
